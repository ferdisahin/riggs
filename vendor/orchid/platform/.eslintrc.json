{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "class-methods-use-this": 0,
        "radix": ["error", "as-needed"]
    },
    "extends": "airbnb-base",
    "globals": {
        "axios": true,
        "Turbolinks": true
    }
}
